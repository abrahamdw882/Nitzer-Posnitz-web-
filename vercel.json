{
  "version": 2,
  "builds": [
    {
      "src": "wed_project/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "wed_project/wsgi.py"
    }
  ],
  "build": {
    "env": {
      "DJANGO_SETTINGS_MODULE": "wed_project.settings",
      "PYTHON_VERSION": "3.12"
    },
    "commands": [
      "pip install -r requirements.txt",
      "python manage.py collectstatic --noinput"
    ]
  }
}
